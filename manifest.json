{
  "manifest_version": 3,
  "name": "Snake",
  "version": "1.0.0",
  "description": "Snake extension pop up game!",
  "action":{
    "default_popup":"index.html",
    "default_icon": {
      "16":"icons/icon.png",
      "48":"icons/icon.png",
      "128":"icons/icon.png"
    }
  },

"chrome_url_overrides": {
    "newtab": "index.html"
},
  "icons":{
    "16":"icons/icon.png",
    "48":"icons/icon.png",
    "128":"icons/icon.png"
  }
}
  
